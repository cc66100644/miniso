<!--miniprogram/pages/image/index.wxml-->
<image-cropper id="image-cropper" 
limit_move="{{true}}" 
disable_rotate="{{true}}"
width="{{width}}"
disable_width = "true" 
disable_height = "true"
height="{{height}}" 
imgSrc="{{src}}" 
bindload="cropperload"
bindimageload="loadimage" bindtapcut="clickcut">
</image-cropper>
