.domes_title {
    font-size: 14pt;
    padding:0 10px 20px 10px;
    margin: 10% 0 0 0;
    text-align: left;
    overflow: hidden;
    /* font-weight: 400; */
    /* white-space: wrap; */
    width: 100%;
    height:70px;
    box-sizing: border-box;
}
.domes_title textarea{
  width: 100%;
}
.domes_title view{
  position: absolute;
  top: 0;
  right: 0;
  color: #999;
  font-size: 12pt;
  margin-right: 20rpx;
}
.ui_cell{
    display: flex;
    position: relative;
    height: 100rpx;
    line-height: 60rpx;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid #eee;
    transform:translate3d(0rpx, 0px, 0px);
}
.ui_cell_bd{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.ui_cell_ft{
    color: #999;
    text-align: right;
}
.ui_uploader_cell {
    width: 100%;
    padding: 40rpx;
    box-sizing: border-box;
}
.ui_uploader_item{
    float: left;
    position: relative;
    margin-right: 30rpx;
    margin-bottom: 30rpx;
    width: 165rpx;
    height: 165rpx;
}
.ui_uploader {
    float: left;
    position: relative;
    margin-right: 25rpx;
    margin-bottom: 25rpx;
    width: 165rpx;
    height: 165rpx;
    border: 2rpx solid #d9d9d9;
    box-sizing: border-box;
}

.ui_uploader:before {
    content: " ";
    position: absolute;
    width: 4rpx;
    height: 79rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #d9d9d9;
}

.ui_uploader:after {
    content: " ";
    position: absolute;
    height: 4rpx;
    width: 79rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #d9d9d9;
}
.ui_uploader_item_icon{
    position: absolute;
    right: -20rpx;
    top: -20rpx;
    background: #fff;
    border-radius: 50%;
}
.ui_uploader_item image {
    width: 100%;
    height: 100%;
}
.btn{
  width: 90%;
  margin: 0 5%;
  border-radius: 20px;
  background-color: red;
  border:1px solid red;
  color: white;
  position: absolute;
  bottom: 5px;
}